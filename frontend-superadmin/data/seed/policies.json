  {
    "id": "POL-010",
    "name": "Inactive User Access Revocation",
    "description": "Automatically revoke access for inactive users",
    "type": "deny",
    "blueprint": "all",
    "status": "active",
    "priority": 1,
    "conditions": [
      {
        "field": "user_status",
        "operator": "equals",
        "value": "inactive"
      },
      {
        "field": "last_login",
        "operator": "older_than",
        "value": "90_days"
      }
    ],
    "actions": ["all"],
    "resources": ["all_resources"],
    "appliedToRoles": ["all"],
    "createdAt": "2024-02-05T10:00:00Z",
    "updatedAt": "2024-02-05T10:00:00Z"
  }
